  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/preciso/issues"
  },
  "homepage": "https://github.com/DanielJDufour/preciso#readme",
  "devDependencies": {
    "flug": "^2.2.0"
  }
