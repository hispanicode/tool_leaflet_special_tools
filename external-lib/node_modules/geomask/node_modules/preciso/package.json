{
  "name": "preciso",
  "version": "0.2.0",
  "description": "Precise Mathematical Functions",
  "main": "preciso.js",
  "unpkg": "preciso.min.js",
  "jsdelivr": "preciso.min.js",
  "files": [
    "absolute.js",
    "add.js",
    "clean.js",
    "compare.js",
    "compare_positive.js",
    "divide.js",
    "expand.js",
    "long_addition.js",
    "long_division.js",
    "long_multiplication.js",
    "long_subtraction.js",
    "max.js",
    "min.js",
    "multiply.js",
    "preciso.js",
    "preciso.min.js",
    "remainder.js",
    "round_last_decimal.js",
    "subtract.js",
    "truncate.js"
  ],
  "scripts": {
    "build": "npx browserify preciso.js > preciso.min.js",
    "format": "npx prettier --arrow-parens=avoid --print-width=160 --trailing-comma=none --write *.js",
    "perf": "node perf.js",
    "prepublishOnly": "npx pkg-ok",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/preciso.git"
  },
  "keywords": [
    "accurate",
    "functions",
    "math",
    "precise"
